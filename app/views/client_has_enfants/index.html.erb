<h1>Client Has Enfants</h1>

<table>
  <thead>
    <tr>
      <th>Enfant</th>
      <th>Client</th>
      <th>Lien</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @client_has_enfants.each do |client_has_enfant| %>
      <tr>
        <td><%= client_has_enfant.Enfant %></td>
        <td><%= client_has_enfant.Client %></td>
        <td><%= client_has_enfant.Lien %></td>
        <td><%= link_to 'Show', client_has_enfant %></td>
        <td><%= link_to 'Edit', edit_client_has_enfant_path(client_has_enfant) %></td>
        <td><%= link_to 'Destroy', client_has_enfant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

